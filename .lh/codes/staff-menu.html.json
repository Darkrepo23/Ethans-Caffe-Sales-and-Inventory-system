            "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n    <meta charset=\"UTF-8\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n    <title>Menu Sales - Staff Dashboard</title>\r\n\r\n    <!-- Bootstrap 5 CSS -->\r\n    <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css\" rel=\"stylesheet\">\r\n    <!-- Font Awesome -->\r\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css\">\r\n    <!-- Animate.css -->\r\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css\">\r\n\r\n    <!-- Custom CSS -->\r\n    <link rel=\"stylesheet\" href=\"style.css\">\r\n    <link rel=\"stylesheet\" href=\"staff.css\">\r\n    <script>\r\n        // Immediate Sidebar State Initialization\r\n        (function () {\r\n            const html = document.documentElement;\r\n            html.classList.add('initializing');\r\n\r\n            const sidebarState = localStorage.getItem('sidebarActive');\r\n            if (sidebarState === 'true' && window.innerWidth >= 992) {\r\n                html.classList.add('sidebar-active');\r\n            }\r\n\r\n            // Auth Guard (Staff/Cashier version)\r\n            try {\r\n                const role = localStorage.getItem('loggedInRole');\r\n                if (role !== 'staff' && role !== 'cashier') {\r\n                    window.location.href = 'index.html';\r\n                }\r\n            } catch (e) {\r\n                window.location.href = 'index.html';\r\n            }\r\n        })();\r\n    </script>\r\n</head>\r\n\r\n<body>\r\n    <!-- Navigation Bar -->\r\n    <nav class=\"navbar navbar-expand-lg navbar-dark bg-black fixed-top shadow\">\r\n        <div class=\"container-fluid\">\r\n            <!-- Sidebar Toggle (Mobile) -->\r\n            <button class=\"sidebar-toggle mt-1 me-2\" id=\"sidebarToggle\">\r\n                <i class=\"fas fa-bars\"></i>\r\n            </button>\r\n\r\n            <!-- Logo -->\r\n\r\n\r\n            <!-- Mobile User Menu Toggle -->\r\n            <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#headerNav\">\r\n                <span class=\"fas fa-ellipsis-v\"></span>\r\n            </button>\r\n\r\n            <!-- Nav Items -->\r\n            <div class=\"collapse navbar-collapse\" id=\"headerNav\">\r\n                <ul class=\"navbar-nav ms-auto\">\r\n                    <!-- User Dropdown -->\r\n                    <li class=\"nav-item dropdown\">\r\n                        <a class=\"nav-link dropdown-toggle\" href=\"#\" role=\"button\" data-bs-toggle=\"dropdown\">\r\n                            <i class=\"fas fa-user-circle me-1\"></i>John Doe\r\n                        </a>\r\n                        <ul class=\"dropdown-menu dropdown-menu-end shadow border-0 animate__animated animate__fadeIn\">\r\n                            <li><a class=\"dropdown-item\" href=\"staff-account.html\"><i\r\n                                        class=\"fas fa-user-cog me-2 text-maroon\"></i>Account</a></li>\r\n                            <li><a class=\"dropdown-item\" href=\"staff-activity.html\"><i\r\n                                        class=\"fas fa-history me-2 text-maroon\"></i>Activity Log</a></li>\r\n                            <li>\r\n                                <hr class=\"dropdown-divider\">\r\n                            </li>\r\n                            <li><a class=\"dropdown-item text-danger\" href=\"#\" id=\"logoutBtn\"><i\r\n                                        class=\"fas fa-sign-out-alt me-2\"></i>Logout</a></li>\r\n                        </ul>\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    </nav>\r\n\r\n    <div class=\"dashboard-layout\">\r\n        <!-- Sidebar Navigation -->\r\n        <aside class=\"sidebar\" id=\"sidebar\">\r\n            <div class=\"sidebar-branding\">\r\n                <button class=\"sidebar-toggle in-sidebar\" id=\"sidebarToggleInternal\">\r\n                    <i class=\"fas fa-bars\"></i>\r\n                </button>\r\n                <a href=\"staff-dashboard.html\" class=\"sidebar-brand-group\">\r\n                    <img src=\"resources/ethans logo.jpg\" alt=\"Logo\">\r\n                    <span class=\"pos-label\">POS</span>\r\n                </a>\r\n            </div>\r\n            <div class=\"sidebar-nav\">\r\n                <div class=\"sidebar-section-title\">Operations</div>\r\n                <a href=\"staff-menu.html\" class=\"sidebar-link active\">\r\n                    <i class=\"fas fa-utensils\"></i>Menu Sales\r\n                </a>\r\n                <a href=\"staff-held-orders.html\" class=\"sidebar-link\">\r\n                    <i class=\"fas fa-pause-circle\"></i>Held Orders\r\n                </a>\r\n                <a href=\"staff-ingredients.html\" class=\"sidebar-link\">\r\n                    <i class=\"fas fa-boxes\"></i>Ingredients\r\n                </a>\r\n                <a href=\"staff-receipts.html\" class=\"sidebar-link\">\r\n                    <i class=\"fas fa-receipt\"></i>Receipts\r\n                </a>\r\n\r\n                <div class=\"sidebar-divider\"></div>\r\n                <div class=\"sidebar-section-title\">Account</div>\r\n                <a href=\"staff-account.html\" class=\"sidebar-link\">\r\n                    <i class=\"fas fa-user-cog\"></i>Account Settings\r\n                </a>\r\n                <a href=\"staff-activity.html\" class=\"sidebar-link\">\r\n                    <i class=\"fas fa-history\"></i>My Activity\r\n                </a>\r\n            </div>\r\n        </aside>\r\n\r\n        <!-- Main Content Area -->\r\n        <main class=\"main-content\">\r\n            <!-- Menu Sales Content -->\r\n            <div id=\"menu-content\" class=\"page-content h-100\">\r\n                <div class=\"row g-0 h-100\">\r\n                    <!-- Left Section: Categories and Products -->\r\n                    <div class=\"col-lg-8 col-md-7 d-flex flex-column border-end overflow-hidden\"\r\n                        style=\"height: calc(100vh - 80px);\">\r\n                        <!-- Categories Header -->\r\n                        <div class=\"p-3 bg-light border-bottom\">\r\n                            <div class=\"d-flex align-items-center mb-3\">\r\n                                <h1 class=\"h4 mb-0 me-4\"><i class=\"fas fa-utensils me-2 text-maroon\"></i>Menu Sales</h1>\r\n                                <div class=\"input-group search-pos\">\r\n                                    <span class=\"input-group-text bg-white border-end-0\"><i\r\n                                            class=\"fas fa-search text-muted\"></i></span>\r\n                                    <input type=\"text\" class=\"form-control border-start-0 ps-0\" id=\"menuSearch\"\r\n                                        placeholder=\"Search menu items or scan barcode...\">\r\n                                </div>\r\n                            </div>\r\n                            <!-- Category Tabs -->\r\n                            <div class=\"category-tabs-container\">\r\n                                <div class=\"category-tabs d-flex gap-2 overflow-auto pb-1\" id=\"categoryTabs\">\r\n                                    <button class=\"btn btn-category active\" data-category=\"All\">All Products</button>\r\n                                    <button class=\"btn btn-category\" data-category=\"Main Course\">Main Course</button>\r\n                                    <button class=\"btn btn-category\" data-category=\"Appetizer\">Appetizer</button>\r\n                                    <button class=\"btn btn-category\" data-category=\"Dessert\">Dessert</button>\r\n                                    <button class=\"btn btn-category\" data-category=\"Beverage\">Drinks</button>\r\n                                    <button class=\"btn btn-category\" data-category=\"Side Dish\">Side Dish</button>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <!-- Menu Items Grid -->\r\n                        <div class=\"flex-grow-1 overflow-auto p-3 bg-white\" id=\"menuItemsGridContainer\">\r\n                            <div class=\"row g-3\" id=\"menuItemsGrid\">\r\n                                <!-- Items will be dynamically loaded here -->\r\n                                <div class=\"col-12 text-center py-5\">\r\n                                    <div class=\"loading-spinner\"></div>\r\n                                    <p class=\"mt-2\">Loading inventory...</p>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <!-- Right Section: Cart Sidebar -->\r\n                    <div class=\"col-lg-4 col-md-5 d-flex flex-column bg-white shadow-sm\"\r\n                        style=\"height: calc(100vh - 80px);\">\r\n                        <!-- Cart Header -->\r\n                        <div class=\"p-3 bg-black text-white\">\r\n                            <div class=\"d-flex justify-content-between align-items-center mb-1\">\r\n                                <h6 class=\"mb-0 fw-bold\"><i class=\"fas fa-shopping-cart me-2\"></i>Cart Details (<span\r\n                                        id=\"cartCount\">0</span>)</h6>\r\n                                <button class=\"btn btn-sm btn-outline-light\" id=\"clearSaleBtn\">\r\n                                    <i class=\"fas fa-trash-alt\"></i>\r\n                                </button>\r\n                            </div>\r\n                            <div class=\"d-flex justify-content-between align-items-center\">\r\n                                <div class=\"small opacity-75\">\r\n                                    <i class=\"fas fa-user-circle me-1\"></i> <span\r\n                                        class=\"customer-name-display text-danger fw-bold\">Walk-in Customer</span>\r\n                                </div>\r\n                                <!-- Order Type Toggle -->\r\n                                <div class=\"btn-group btn-group-sm\" role=\"group\" id=\"orderTypeToggle\">\r\n                                    <input type=\"radio\" class=\"btn-check\" name=\"orderType\" id=\"orderTypeDineIn\" value=\"dine-in\" autocomplete=\"off\">\r\n                                    <label class=\"btn btn-outline-warning btn-sm py-0 px-2\" for=\"orderTypeDineIn\"><i class=\"fas fa-chair me-1\"></i>Dine-in</label>\r\n                                    \r\n                                    <input type=\"radio\" class=\"btn-check\" name=\"orderType\" id=\"orderTypeWalkIn\" value=\"walk-in\" autocomplete=\"off\" checked>\r\n                                    <label class=\"btn btn-outline-warning btn-sm py-0 px-2\" for=\"orderTypeWalkIn\"><i class=\"fas fa-walking me-1\"></i>Walk-in</label>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <!-- Cart Items List -->\r\n                        <div class=\"flex-grow-1 overflow-auto p-2\" id=\"cartItemsList\">\r\n                            <div class=\"text-center py-5 empty-cart-msg\">\r\n                                <i class=\"fas fa-cart-plus fa-3x text-light mb-3\" style=\"color: #eee !important;\"></i>\r\n                                <p class=\"text-muted\">No items added to sale yet.</p>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <!-- Price Section -->\r\n                        <div class=\"p-3 border-top bg-light\">\r\n                            <div class=\"d-flex justify-content-between mb-1\">\r\n                                <span class=\"text-muted\">Sub-total</span>\r\n                                <span class=\"fw-bold\" id=\"cartSubtotal\">P0.00</span>\r\n                            </div>\r\n                            <div class=\"d-flex justify-content-between mb-1\">\r\n                                <span class=\"text-muted\">Taxes (12%)</span>\r\n                                <span class=\"fw-bold\" id=\"cartTaxes\">P0.00</span>\r\n                            </div>\r\n                            <hr class=\"my-2\">\r\n                            <div class=\"d-flex justify-content-between mb-3\">\r\n                                <span class=\"h5 fw-bold mb-0\">Total</span>\r\n                                <span class=\"h5 fw-bold mb-0 text-maroon\" id=\"cartTotal\">P0.00</span>\r\n                            </div>\r\n\r\n                            <!-- POS Action Buttons -->\r\n                            <div class=\"pos-actions grid gap-2\">\r\n                                <button class=\"btn btn-outline-dark w-100 mb-2\" id=\"selectCustomerBtn\">\r\n                                    <i class=\"fas fa-user-plus me-2\"></i>Select Customer\r\n                                </button>\r\n                                <div class=\"row g-2 mb-2\">\r\n                                    <div class=\"col-6\">\r\n                                        <button class=\"btn btn-outline-danger w-100\" id=\"discountBtn\">\r\n                                            <i class=\"fas fa-minus-circle me-1\"></i>Discount\r\n                                        </button>\r\n                                    </div>\r\n                                    <div class=\"col-6\">\r\n                                        <button class=\"btn btn-outline-primary w-100\" id=\"couponBtn\">\r\n                                            <i class=\"fas fa-tag me-1\"></i>Coupon\r\n                                        </button>\r\n                                    </div>\r\n                                </div>\r\n                                <button class=\"btn btn-maroon btn-lg w-100 mb-2 py-3 fw-bold shadow-sm\" id=\"checkoutBtn\"\r\n                                    disabled>\r\n                                    <i class=\"fas fa-cash-register me-2\"></i>CHECKOUT\r\n                                </button>\r\n                                <div class=\"row g-2\">\r\n                                    <div class=\"col-6\">\r\n                                        <button class=\"btn btn-warning w-100 text-white\" id=\"holdOrderBtn\">\r\n                                            <i class=\"fas fa-pause-circle me-1\"></i>Hold Order\r\n                                        </button>\r\n                                    </div>\r\n                                    <div class=\"col-6\">\r\n                                        <button class=\"btn btn-secondary w-100\" id=\"returnOrderBtn\">\r\n                                            <i class=\"fas fa-reply me-1\"></i>Return\r\n                                        </button>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </main>\r\n    </div>\r\n\r\n    <!-- Notification Modal -->\r\n    <div class=\"modal fade\" id=\"notificationModal\" tabindex=\"-1\">\r\n        <div class=\"modal-dialog\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header\" id=\"notificationModalHeader\">\r\n                    <h5 class=\"modal-title\" id=\"notificationModalTitle\"></h5>\r\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\"></button>\r\n                </div>\r\n                <div class=\"modal-body\" id=\"notificationModalBody\">\r\n                </div>\r\n                <div class=\"modal-footer\">\r\n                    <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>\r\n                    <button type=\"button\" class=\"btn btn-primary\" id=\"notificationModalConfirm\"\r\n                        style=\"display: none;\">Confirm</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- Bootstrap 5 JS Bundle with Popper -->\r\n    <script src=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js\"></script>\r\n    <!-- jQuery for AJAX -->\r\n    <script src=\"https://code.jquery.com/jquery-3.6.4.min.js\"></script>\r\n    <!-- SweetAlert2 for nicer alerts and confirmations -->\r\n    <script src=\"https://cdn.jsdelivr.net/npm/sweetalert2@11\"></script>\r\n\r\n    <!-- Custom JS -->\r\n    <script src=\"main.js\"></script>\r\n    <script src=\"staff.js\"></script>\r\n\r\n    <script>\r\n        // Centralized logic handles these features\r\n    </script>\r\n</body>\r\n\r\n</html>"
        }
